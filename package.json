{
  "name": "vidcall-platform",
  "version": "1.0.0",
  "description": "Doctor-Patient Video Call Platform using Agora.io",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server.js",
    "client": "cd client && npm start",
    "build": "npm install && cd client && npm install && npm run build",
    "install-all": "npm install && cd client && npm install",
    "setup": "node setup.js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "agora-access-token": "^2.0.4",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.0"
  },
  "keywords": ["video-call", "agora", "react", "nodejs", "doctor-patient"],
  "author": "",
  "license": "MIT"
}
